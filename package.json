{
  "name": "my-electron-react-ts",
  "version": "1.0.0",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "node scripts/dev.js",
    "dev:concurrent": "concurrently \"npm run dev:renderer\" \"npm run dev:main\"",
    "dev:renderer": "vite",
    "dev:main": "tsc -p tsconfig.main.json && cross-env NODE_ENV=development electron .",
    "build": "npm run build:renderer && npm run build:main",
    "build:renderer": "vite build",
    "build:main": "tsc -p tsconfig.main.json",
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "electron",
    "react",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "description": "Electron app with React and TypeScript",
  "dependencies": {
    "@types/file-saver": "^2.0.7",
    "file-saver": "^2.0.5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@electron/remote": "^2.1.3",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "electron": "^37.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.1"
  }
}
